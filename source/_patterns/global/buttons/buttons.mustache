                <div class="mainBlock__inner__order">
                    Order Type
                </div>

                <label class="mainBlock__selectLabel">
                    <select class="mainBlock__select">
                        <optgroup label="Select">
                            <option> One-Cancels-the-Other (OCO)</option>
                            <!--class="drop-down__content"-->
                            <option>Option Two</option>
                            <option>Option Three</option>
                        </optgroup>
                    </select>
                </label>
            </div>


            <table>
                <tr>
                    <td class="mainBlock__td mainBlock__td-fontStyle-1">Take Profit</td>
